@page
@model IndexModel
@{
    ViewData["Title"] = "Auth";
}

<div class="text-center">
    <h1 class="display-4">Auth</h1>
    <component type="typeof(Auth.Component)" render-mode="Server" />
    @await Component.InvokeAsync(
        "QRCode",
        new {
            data = @Model.uri?.ToString()
        }
    )
</div>




